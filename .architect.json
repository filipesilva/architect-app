{
  "$schema": "./node_modules/@angular-devkit/architect/src/workspace-schema.json",
  "name": "pet-finder",
  "version": 1,
  "defaultProject": "myApp",
  "root": "./",
  "projects": {
    "myApp": {
      "root": "./projects/architect-demo",
      "projectType": "application",
      "defaultTarget": "browser",
      "targets": {
        "browser": {
          "builder": "@angular-devkit/build-webpack:browser",
          "options": {
            "tsConfig": "tsconfig.app.json",
            "outputPath": "dist",
            "index": "index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts"
          },
          "configurations": {
            "production": {
              "aot": true,
              "optimizationLevel": 1
            }
          }
        },
        "devServer": {
          "builder": "@angular-devkit/build-webpack:devServer",
          "options": {
            "browserTarget": "myApp:browser"
          },
          "configurations": {
            "production": {
              "browserTarget": "myApp:browser:production"
            }
          }
        },
        "extractI18n": {
          "builder": "@angular-devkit/build-webpack:extractI18n",
          "options": {
            "browserTarget": "myApp:browser"
          }
        },
        "karma": {
          "builder": "@angular-devkit/build-webpack:karma",
          "options": {
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "progress": false
          }
        },
        "tslint": {
          "builder": "@angular-devkit/build-webpack:tslint",
          "options": {
            "tslintConfig": "tslint.json",
            "tsConfig": "tsconfig.app.json"
          }
        }
      }
    },
    "e2e": {
      "root": "./projects/e2e",
      "projectType": "application",
      "defaultTarget": "browser",
      "targets": {
        "browser": {
          "builder": "@angular-devkit/build-webpack:protractor",
          "options": {
            "protractorConfig": "protractor.conf.js",
            "devServerTarget": "myApp:devServer"
          }
        }
      }
    }
  }
}
